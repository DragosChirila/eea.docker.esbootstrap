block title
    title!= head_title

block extrajavascripts
    each script in js_resources
        script(type='text/javascript', src=script)

block extrastyles
    link(rel='stylesheet', href='css/no-more-tables.css')
    each style in css_resources
        link(rel='stylesheet', href=style)

block content
    +portal_breadcrumbs (breadcrumbs)
    h1!= site_title
    p!= site_description
    .facet-view-simple

    +visualization_info (dataprovencance_info_url,dataprovencance_info_text)

    div!= further_info
